<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />

	<!-- Definition for profile bean -->
	<bean id="profile"
		class="com.spring_core_autowire_qualifier_annotation.model.Profile"></bean>

	<!-- Definition for student1 bean -->
	<bean id="student1"
		class="com.spring_core_autowire_qualifier_annotation.model.Student">
		<property name="name" value="Student1" />
		<property name="age" value="10" />
	</bean>

	<!-- Definition for student2 bean -->
	<bean id="student2"
		class="com.spring_core_autowire_qualifier_annotation.model.Student">
		<property name="name" value="Student2" />
		<property name="age" value="20" />
	</bean>

</beans>